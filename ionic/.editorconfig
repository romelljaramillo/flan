# Editor configuration, see https://editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.ts]
indent_style = space
indent_size = 2

[*.html]
indent_style = space
indent_size = 2

[*.scss]
indent_style = space
indent_size = 2
